// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model msevents {
  id        Int      @id @default(autoincrement())
  title      String
  date      String
  location  String
  Body      String
  time      String
  published Boolean  @default(false)
}

model ylcevents {
  id        Int      @id @default(autoincrement())
  title      String
  date      String
  location  String
  Body      String
  time      String
  published Boolean  @default(false)
}

model msnews {
  id        Int      @id @default(autoincrement())
  title      String
  date      DateTime @default(now())
  location  String
  Body      String
  published Boolean  @default(false)
}

model ylcnews {
  id        Int      @id @default(autoincrement())
  title      String
  date      DateTime @default(now())
  location  String
  Body      String
  published Boolean  @default(false)
}

model msofficers {
  id        Int      @id @default(autoincrement())
  title      String
  image    String
  Body      String
  role     String
}

model ylcofficers {
  id        Int      @id @default(autoincrement())
  title      String
  image    String
  Body      String
  role     String
}